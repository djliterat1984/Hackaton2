<!-- views/partials/student-list.ejs -->
<table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Debt</th>
      </tr>
    </thead>
    <tbody>
      <% students.forEach(student => { %>
        <tr>
          <td><%= student.id %></td>
          <td><%= student.name %></td>
          <td><%= student.debt %></td>
        </tr>
      <% }) %>
    </tbody>
</table>  