<!-- views/partials/payment-list.ejs -->
<table>
    <thead>
      <tr>
        <th>Payment ID</th>
        <th>Student ID</th>
        <th>Method ID</th>
        <th>Amount</th>
      </tr>
    </thead>
    <tbody>
      <% payments.forEach(payment => { %>
        <tr>
          <td><%= payment.payment_id %></td>
          <td><%= payment.student_id %></td>
          <td><%= payment.method_id %></td>
          <td><%= payment.amount %></td>
        </tr>
      <% }) %>
    </tbody>
</table>  